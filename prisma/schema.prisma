generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("POSTGRES_URL")
}

model team {
  id Int @id @default(autoincrement())
  name String @db.VarChar()
  drivers String @db.VarChar()
  champions Int @db.Integer()
}
